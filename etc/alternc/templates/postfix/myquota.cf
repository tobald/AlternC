# WARNING: Do not edit this file, edit the one in /etc/alternc/templates and launch alternc.install again.
# Get the quota
#

user = %%db_mail_user%%
password = %%db_mail_pwd%%
hosts =%%dbhost%%
dbname = %%dbname%%
query = select quota from mailbox join address on mailbox.id = address.id join domaines on domaines.id = address.domain_id where concat(address.address,'@',domaines.domaine) = '%s'

